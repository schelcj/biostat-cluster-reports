<workbook>
  <worksheet name="Total Cluster Utilization">
    <format bg_color="silver" bold="1">
      <row>
        <cell width="45%"><var name="caption"/></cell>
        <cell />
      </row>
      <row>
        <cell>Time</cell>
        <cell width="25%">Percentage of Reported Time</cell>
      </row>
    </format>

    <loop name="results">
      <row>
        <cell><var name="header"/></cell>
        <cell><var name="percent"/>%</cell> 
      </row>
    </loop>
  </worksheet>
</workbook>

