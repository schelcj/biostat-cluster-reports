<workbook>
  <worksheet name="Top Usage by User">
    <format bg_color="silver" bold="1">
      <row>
        <cell>Top User Usage - Last 90 days</cell>
        <cell />
        <cell />
      </row>
      <row>
        <cell>User</cell>
        <cell>Account</cell>
        <cell>CPU Minutes</cell> 
      </row>
    </format>

    <loop name="results">
      <row>
        <cell><var name="name"/> (<var name="login"/>)</cell>
        <cell><var name="account"/></cell>
        <cell><var name="cpu_min"/></cell> 
      </row>
    </loop>
  </worksheet>
</workbook>

